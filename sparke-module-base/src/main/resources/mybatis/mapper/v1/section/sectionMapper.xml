<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.sparke.base.modules.v1.fragment.mapper.SectionMapper">
    <select id="findList" resultType="cn.sparke.base.modules.v1.fragment.bean.SectionBean">
        SELECT
            name,
            code
        FROM tb_section
        ORDER BY sort
    </select>
</mapper>